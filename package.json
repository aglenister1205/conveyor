{
  "name": "@autoinvent/conveyor",
  "version": "1.0.0-beta.2",
  "description": "GraphiQL for magql",
  "license": "BlueOak-1.0.0",
  "author": "Moebius Solutions",
  "files": [
    "dist"
  ],
  "main": "./dist/conveyor.umd.js",
  "module": "./dist/conveyor.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/conveyor.mjs",
      "require": "./dist/conveyor.umd.js"
    },
    "./dist/styles/index.css": {
      "import": "./dist/styles/index.css",
      "require": "./dist/styles/index.css"
    }
  },
  "scripts": {
    "example": "cd example && vite",
    "build": "tsc && vite build",
    "format": "rome format . --write",
    "run-tsc": "tsc",
    "run-rome-lint": "rome check . --apply",
    "run-stylelint": "stylelint --cache --fix --ignore-path .gitignore '**/*.css'",
    "lint": "run-p run-tsc run-stylelint run-rome-lint",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:ui": "vitest --ui",
    "validate": "run-p lint test:ci",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.28.4",
    "@tanstack/react-router": "^1.28.4",
    "@tanstack/react-store": "^0.3.1",
    "react-bootstrap": "^2.10.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.10.1",
    "react-use": "^17.4.0"
  },
  "peerDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@tanstack/react-query-devtools": "^4.32.0",
    "@testing-library/dom": "9.3.1",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-helmet": "6.1.6",
    "@types/testing-library__jest-dom": "5.14.8",
    "@vitejs/plugin-react": "4.0.3",
    "@vitest/ui": "0.33.0",
    "bootstrap": "^5.3.3",
    "graphql-request": "^6.1.0",
    "jsdom": "22.1.0",
    "lint-staged": "13.2.3",
    "npm-run-all": "4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rome": "12.1.3",
    "storybook": "^7.6.17",
    "stylelint": "15.10.2",
    "stylelint-config-standard": "34.0.0",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vite-plugin-dts": "^3.3.1",
    "vite-plugin-node-polyfills": "^0.11.2",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "0.33.0"
  },
  "lint-staged": {
    "*": "rome format . --write",
    "*.css": "stylelint --cache --fix",
    "*.{ts,tsx}": [
      "rome check --apply",
      "vitest related --run --coverage=false"
    ]
  }
}